mixin s-contact()
	// start s-contact
	+b.s-contact.section.block-with-lazy#s-contact&attributes(attributes)
		.container
			+b.section-title.d-md-none.text-center
						h2 	Приезжайте к&nbsp;нам
							| 
							strong.text-primary  на&nbsp;производство, 
							| 
							| 
						+e.text чтобы лично проверить качество шлакоблока и&nbsp;обсудить поставку
			.row
				.col-md-6.align-self-end
					
					script.
						var map;
						function initMap() {
								map = new google.maps.Map(document.getElementById('map'), {
								zoom: 15,
								//- center: new google.maps.LatLng(53.98205856875258,88.80421049999992),
								center: new google.maps.LatLng(53.38863407106368,59.082403),
								mapTypeId: 'roadmap',
								styles: [
									{
											"featureType": "all",
											"elementType": "all",
											"stylers": [
													{
															"hue": "#ff0000"
													},
													{
															"gamma": "0.00"
													},
													{
															"lightness": "0"
													},
													{
															"saturation": "1"
													}
											]
									},
									{
											"featureType": "administrative",
											"elementType": "labels.text.fill",
											"stylers": [
													{
															"color": "#444444"
													}
											]
									},
									{
											"featureType": "landscape",
											"elementType": "all",
											"stylers": [
													{
															"color": "#f2f2f2"
													}
											]
									},
									{
											"featureType": "poi",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "road",
											"elementType": "all",
											"stylers": [
													{
															"saturation": -100
													},
													{
															"lightness": 45
													}
											]
									},
									{
											"featureType": "road.highway",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "simplified"
													}
											]
									},
									{
											"featureType": "road.arterial",
											"elementType": "labels.icon",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "transit",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "water",
											"elementType": "all",
											"stylers": [
													{
															"color": "#ffffff"
													},
													{
															"visibility": "on"
													}
											]
									}
							]
						});
						var iconBase = 'img/svg/mark.svg';
						var icons = {
					 
						info: {
						icon: iconBase
						}
						};
						var features = [
						{
						position: new google.maps.LatLng(53.38863407106368,59.082403),
						type: 'info',
						title: 'г. Магнитогорск',
						}, 
						//- {
						//- position: new google.maps.LatLng(54.999397069703306,82.95985649999986),
						//- type: 'info',
						//- title: 'г. Новосибирск',
						//- },  
						];
						// Create markers.
						features.forEach(function(feature) {
						var marker = new google.maps.Marker({
						position: feature.position,
						icon: icons[feature.type].icon,
						map: map,
						title: feature.title,
						//- title: title
						});
						});
						}
					//- script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=key=AIzaSyCnoRp8wBQeCZLL8k4R-vckP_wDGVR2k10&callback=initMap')
					script(async='', defer='' src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCnoRp8wBQeCZLL8k4R-vckP_wDGVR2k10&callback=initMap')
					+e.map-wrap.lazy-background
						+e.img-wrap--lg(data-fancybox="gal-cont" href="img/@2x/fasad-2.jpg")
							+pic("fasad-1")(class='picture-bg')
						#map
				+e.col.col-md-6.order-md-first
					+b.section-title.d-none.d-md-block
						h2 Приезжайте к&nbsp;нам
							| 
							strong.text-primary   на&nbsp;производство, 
							| 
						+e.text чтобы лично проверить качество шлакоблока и&nbsp;обсудить поставку
					table
						tr
							td
								+e.icon-wrap
									+icon('mark')
							+e.TD.title Адрес:
								
							+e.TD.text Магнитогорск, ул.Вайнера, д.2А
						
						tr
							td
								+e.icon-wrap
									+icon('telephone')
							+e.TD.title Телефон:
								
							+e.TD.text
								a(href="tel:+7(3519)46-02-03") +7 (3519) 46-02-03
						tr
							td
								+e.icon-wrap
									+icon('mail2')
							+e.TD.title Email:
								
							+e.TD.text 
								a(href="mailto:455203@mail.ru") 455203@mail.ru
						tr
							td
								+e.icon-wrap
									+icon('clock')
							+e.TD.title Работаем
								+e.small ежедневно
								
							+e.TD.text с 9:00 до 20:00
				
	// end s-contact